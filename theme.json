{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
                {
					"color": "#b50600",
					"name": "Brand Primary",
					"slug": "brandPrimary"
				},
                {
					"color": "#7D0700",
					"name": "Brand Secondary",
					"slug": "brandSecondary"
				},
                {
					"color": "#000000",
					"name": "Content Primary",
					"slug": "contentPrimary"
				},
                {
					"color": "#444342",
					"name": "Content Secondary",
					"slug": "contentSecondary"
				},
                {
					"color": "#292f36",
					"name": "Content Tertiary",
					"slug": "contentTertiary"
				},
                {
					"color": "#dddddd",
					"name": "Detail Primary",
					"slug": "detailPrimary"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#f9285a",
					"name": "Brand Primary Light 1",
					"slug": "brandPrimaryLight1"
				}
			]
		},
        "useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "645px",
			"wideSize": "1340px"
		},
        "dimensions": {
			"minHeight": true
		},
        "position": {
			"sticky": true
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "10px",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "20px",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "30px",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "clamp(30px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(30px, 7vw, 70px)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(50px, 7vw, 90px)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"writingMode": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.38rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.15rem"
					},
					"name": "Extra Extra Large",
					"size": "2.15rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"name": "Headings",
					"slug": "headings",
					"fontFamily": "PoppinsBold, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/Poppins/Poppins-Bold.ttf"
							],
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontFamily": "PoppinsBold"
						}
					]
				},
				{
					"name": "DecorativeHeadings",
					"slug": "decorative",
					"fontFamily": "DecorativeHeadings, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/PlayfairDisplay/PlayfairDisplay-Regular.ttf"
							],
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "DecorativeHeadings"
						}
					]
				},
                {
					"name": "Body",
					"slug": "body",
					"fontFamily": "Poppins, sans-serif",
					"fontFace": [
						{
                            "fontFamily": "Poppins",
							"fontWeight": "400",
							"fontStyle": "normal",
                            "src": [
								"file:./assets/fonts/Poppins/Poppins-Regular.ttf"
							]
						},
                        {
                            "fontFamily": "Poppins",
							"fontWeight": "400",
							"fontStyle": "italic",
                            "src": [
								"file:./assets/fonts/Poppins/Poppins-Italic.ttf"
							]
						},
                        {
                            "fontFamily": "Poppins",
							"fontWeight": "700",
							"fontStyle": "normal",
                            "src": [
								"file:./assets/fonts/Poppins/Poppins-Bold.ttf"
							]
						},
                        {
                            "fontFamily": "Poppins",
							"fontWeight": "700",
							"fontStyle": "italic",
                            "src": [
								"file:./assets/fonts/Poppins/Poppins-BoldItalic.ttf"
							]
						}
					]
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var:preset|color|white",
			"text": "var:preset|color|contentPrimary"
		},
        "spacing": {
			"padding": {
				"top": "0",
				"bottom": "0",
				"left": "2rem",
				"right": "2rem"
			}
		},
        "blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"lineHeight": "1.0",
					"textTransform": "uppercase"
				},
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var:preset|font-size|xx-large"
						}
					},
					"h2": {
						"typography": {
							"fontSize": "var:preset|font-size|x-large"
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var:preset|font-size|large"
						}
					},
					"h4": {
						"typography": {
							"fontSize": "var:preset|font-size|medium"
						}
					},
					"h5": {
						"typography": {
							"fontSize": "var:preset|font-size|medium"
						}
					}
				}
			}
		},
        "elements": {}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
	]
}
